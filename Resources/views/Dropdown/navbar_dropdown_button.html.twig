{% extends "BootstrapButtonBlockBundle:Button:dropdown_button_params.html.twig" %}

{% block body %}
<li class="dropdown" {{ editor|raw }}>
    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
        {{ button_text }}
        <span class="caret"></span>
    </a>
    <ul class="dropdown-menu{{ button_dropup_right }}">
        {% include "BootstrapButtonBlockBundle:Item:_dropdown_items.html.twig" with {'items': data.items} %}
    </ul>
</li>
{% endblock %}
